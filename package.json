{
  "name": "@stdlib/math-base-special-asecf",
  "version": "0.1.1",
  "description": "Compute the inverse (arc) secant of a single-precision floating-point number.",
  "license": "Apache-2.0",
  "author": {
    "name": "The Stdlib Authors",
    "url": "https://github.com/stdlib-js/stdlib/graphs/contributors"
  },
  "contributors": [
    {
      "name": "The Stdlib Authors",
      "url": "https://github.com/stdlib-js/stdlib/graphs/contributors"
    }
  ],
  "main": "./lib",
  "gypfile": false,
  "directories": {
    "benchmark": "./benchmark",
    "doc": "./docs",
    "example": "./examples",
    "include": "./include",
    "lib": "./lib",
    "src": "./src",
    "test": "./test"
  },
  "types": "./docs/types",
  "scripts": {
    "test": "make test",
    "test-cov": "make test-cov",
    "examples": "make examples",
    "benchmark": "make benchmark"
  },
  "homepage": "https://stdlib.io",
  "repository": {
    "type": "git",
    "url": "git://github.com/stdlib-js/math-base-special-asecf.git"
  },
  "bugs": {
    "url": "https://github.com/stdlib-js/stdlib/issues"
  },
  "dependencies": {
    "@stdlib/math-base-napi-unary": "^0.2.3",
    "@stdlib/math-base-special-acosf": "^0.1.1",
    "@stdlib/number-float64-base-to-float32": "^0.2.2",
    "@stdlib/utils-library-manifest": "^0.2.2",
    "@stdlib/error-tools-fmtprodmsg": "^0.2.2"
  },
  "devDependencies": {
    "@stdlib/console-log-each-map": "github:stdlib-js/console-log-each-map#main",
    "@stdlib/constants-float32-eps": "^0.2.2",
    "@stdlib/math-base-assert-is-nanf": "^0.2.2",
    "@stdlib/math-base-special-abs": "^0.2.2",
    "@stdlib/random-array-uniform": "^0.2.1",
    "@stdlib/random-base-randu": "^0.2.1",
    "@stdlib/utils-try-require": "^0.2.2",
    "tape": "git+https://github.com/kgryte/tape.git#fix/globby",
    "istanbul": "^0.4.1",
    "tap-min": "git+https://github.com/Planeshifter/tap-min.git",
    "@stdlib/bench-harness": "^0.2.2"
  },
  "engines": {
    "node": ">=0.10.0",
    "npm": ">2.7.0"
  },
  "os": [
    "aix",
    "darwin",
    "freebsd",
    "linux",
    "macos",
    "openbsd",
    "sunos",
    "win32",
    "windows"
  ],
  "keywords": [
    "stdlib",
    "stdmath",
    "mathematics",
    "math",
    "asecf",
    "inverse",
    "cosine",
    "cos",
    "arc",
    "arccosine",
    "secant",
    "arcsecant",
    "trig",
    "trigonometry",
    "angle"
  ],
  "__stdlib__": {
    "scaffold": {
      "$schema": "math/base@v1.0",
      "base_alias": "asec",
      "alias": "asecf",
      "pkg_desc": "compute the inverse (arc) secant of a single-precision floating-point number",
      "desc": "computes the inverse (arc) secant of a single-precision floating-point number",
      "short_desc": "inverse (arc) secant",
      "parameters": [
        {
          "name": "x",
          "desc": "input value",
          "type": {
            "javascript": "number",
            "jsdoc": "number",
            "c": "float",
            "dtype": "float32"
          },
          "domain": [
            {
              "min": "-infinity",
              "max": -1
            },
            {
              "min": 1,
              "max": "infinity"
            }
          ],
          "rand": {
            "prng": "random/base/uniform",
            "parameters": [
              1.1,
              5.1
            ]
          },
          "example_values": [
            905.5,
            -123.75,
            1.001,
            -1.08,
            66.2,
            -201.9,
            13.7,
            -3.81,
            4.43,
            -19.6,
            2.28,
            -7.33,
            150.25,
            -11.2,
            5.04,
            -1.36,
            1.21,
            -88.65,
            37.9,
            -402.3
          ]
        }
      ],
      "output_policy": "real_floating_point_and_generic",
      "returns": {
        "desc": "arcsecant",
        "type": {
          "javascript": "number",
          "jsdoc": "number",
          "c": "float",
          "dtype": "float32"
        }
      },
      "keywords": [
        "asecf",
        "inverse",
        "cosine",
        "cos",
        "arc",
        "arccosine",
        "secant",
        "arcsecant"
      ],
      "extra_keywords": [
        "math.acos"
      ]
    }
  },
  "funding": {
    "type": "opencollective",
    "url": "https://opencollective.com/stdlib"
  }
}